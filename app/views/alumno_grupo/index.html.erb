<h1>AlumnoGrupo#index</h1>
<p>Find me in app/views/alumno_grupo/index.html.erb</p>
<h2>GRUPOS DEL ALUMNO EN DATA WAREHOUSE</h2>
<table style="width:100%", class= "table table-striped table-bordered table-sm">
    <tr>
        <th>Id Grupo</th>
        <th>Id Alumno</th> 
        <th>Oportunidad</th>
        <th>Promedio</th>
    </tr>
    <% @alumno_grupo_data.each do |grupo| %>
        <tr>
            <th><%= grupo.Id_Grupo %></th> 
            <th><%= grupo.Id_Alumno %></th> 
            <th><%= grupo.Oportunidad %></th> 
            <th><%= grupo.Promedio %></th> 
        </tr>
    <%end%>
</table>
