<h1>Aula#index</h1>
<h2>AULAS EN CONTROL ACADEMICO</h2>
<table style="width:100%", class= "table table-striped table-bordered table-sm">
    <tr>
        <th>Id Aula</th>
        <th>Nombre</th> 
        <th>Edificio</th>
    </tr>
    <% @aulasCA.each do |aula| %>
        <tr>
            <th><%= aula.Id_Aula %></th> 
            <th><%= aula.Nombre %></th> 
            <th><%= aula.Edificio %></th> 
        </tr>
    <%end%>
</table>
<h2>AULAS EN EXTRAESCOLARES</h2>
<table style="width:100%", class= "table table-striped table-bordered table-sm">
    <tr>
        <th>Id Aula</th>
        <th>Nombre</th> 
        <th>Descripcion</th>
    </tr>
    <% @aulasE.each do |aula| %>
        <tr>
            <th><%= aula.Id_aula %></th> 
            <th><%= aula.Nombre %></th> 
            <th><%= aula.Descripcion %></th> 
        </tr>
    <%end%>
</table>
