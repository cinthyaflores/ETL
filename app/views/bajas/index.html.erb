<h1 class="text-center">Bajas</h1>
<!----<p>Find me in app/views/bajas/index.html.erb</p>---->
<h2 class="text-center">BAJAS EN DATA WAREHOUSE</h2>
<table style="width:100%", class= "table table-striped table-bordered table-sm">
    <tr>
        <th>Id Baja</th>
        <th>Id Alumno</th> 
        <th>Tipo Baja</th>
        <th>Fecha Baja</th>
    </tr>
    <% @bajas_data.each do |baja| %>
        <tr>
            <th><%= baja.Id_Baja %></th> 
            <th><%= baja.Id_Alumno %></th> 
            <th><%= baja.Id_Tipo_Baja %></th> 
            <th><%= baja.Fecha_Baja %></th> 
        </tr>
    <%end%>
</table>
