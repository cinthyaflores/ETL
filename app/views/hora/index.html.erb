<h1 class="text-center">Hora</h1>
<!----<p>Find me in app/views/hora/index.html.erb</p>---->
<h2 class="text-center">HORARIOS EN DATA WAREHOUSE</h2>
<table style="width:100%", class= "table table-striped table-bordered table-sm">
    <tr>
        <th>Id Horario</th>
        <th>Hora Inicio</th> 
        <th>Hora Fin</th>
    </tr>
    <% @horas_data.each do |hora| %>
        <tr>
            <th><%= hora.Id_Hora %></th> 
            <th><%= hora.Hora_inicio %></th> 
            <th><%= hora.Hora_fin%></th> 
        </tr>
    <%end%>
</table>
